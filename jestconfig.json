{
  "collectCoverage": true,
  "coverageDirectory": "./test/report",
  "coverageThreshold": {
      "global": {
          "branches": 100,
          "functions": 100,
          "lines": 100,
          "statements": 0
      }
  },
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[jt]sx?$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}